{
  "accounts": [
    {
      "name": "DevPersonal",
      "token": "${GITHUB_TOKEN_PERSONAL}",
      "repos": [
        "apache/rocketmq-client-go",
        "facebook/react"
      ],
      "devBranch": "dev",
      "upstreamBranch": "main"
    },
    {
      "name": "WorkAccount",
      "token": "${GITHUB_TOKEN_WORK}",
      "repos": [
        "nodejs/node",
        "nestjs/nest"
      ],
      "devBranch": "develop",
      "upstreamBranch": "master"
    }
  ],
  "email": {
    "smtpHost": "smtp.example.com",
    "smtpPort": 587,
    "user": "noreply@example.com",
    "pass": "${SMTP_PASSWORD}",
    "from": "noreply@example.com",
    "to": [
      "devteam@example.com",
      "manager@example.com"
    ]
  },
  "schedule": "0 4 * * *",
  "logOptions": {
    "level": "${LOG_LEVEL}",
    "outputFile": "logs/sync-log.txt",
    "enableConsole": true
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4",
    "apiKey": "${OPENAI_API_KEY}",
    "maxRetries": 3
  },
  "reposBaseDir": "${REPOS_BASE_DIR}",
  "maxMergeRetries": 3,
  "runTestsAfterMerge": true,
  "sendMailOnFailure": true,
  "sendDailyReport": true
}
